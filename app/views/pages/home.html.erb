<h1>Pages#home</h1>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Category</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
    <tr>
      <td><%= profile.title %></td>
      <td><%= profile.description %></td>
      <td><%= profile.category_id %></td>
      <td><%= profile.user_id %></td>
      <td><%= link_to 'Show', profile %></td>
    </tr>
    <% end %>
  </tbody>
</table>